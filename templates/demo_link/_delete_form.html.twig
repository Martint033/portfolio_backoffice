<form method="post" action="{{ path('demo_link_delete', {'id': demo_link.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demo_link.id) }}">
    <button class="btn">Delete</button>
</form>
